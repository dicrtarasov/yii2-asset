{
    "name": "dicr/yii2-asset",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "Ресурсы библиотек для Yii2",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "http://dicr.org",
            "role": "developer"
        }
    ],
    "keywords": ["dicr", "yii2", "res", "asset"],
    "homepage": "https://gitlab.com/dicr/yii2-asset",
    "support": {
        "source": "https://gitlab.com/dicr/yii2-asset.git"
    },
    "autoload": {
        "psr-4": {
            "dicr\\asset\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "yiisoft/yii2": "~2.0"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ],
    "require-dev": {
        "roave/security-advisories": "dev-master"
    }
}
