{
  "type": "yii2-extension",
  "name": "dicr/yii2-asset",
  "license": "GPL-2.0",
  "description": "AssetBundle helpers for Yii2",
  "authors": [
    {
      "name": "Igor Tarasov",
      "email": "develop@dicr.org",
      "homepage": "http://dicr.org",
      "role": "developer"
    }
  ],
  "keywords": [
    "dicr yii2 asset-bundle"
  ],
  "homepage": "https://gitlab.com/dicr/yii2-asset",
  "support": {
    "source": "https://gitlab.com/dicr/yii2-asset.git"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "yiisoft/yii2": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "dicr\\asset\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "~8.1"
  }
}
